
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Panel de Control | Sistema de Psicologia</title>
    <meta name="description" content="Dashboard UI Kit">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Google Font -->
    
    <!-- Favicon -->
    <link rel="apple-touch-icon" href="apple-touch-icon.png">
    <link rel="icon" href="~/Content/img/favicon.png" type="image/png">
    <!-- Stylesheet -->
    <link rel="stylesheet" href="~/Content/css/main.min.css">
    <link href="~/Content/css/custom.css" rel="stylesheet" />
    <link href="~/Content/css/main.css" rel="stylesheet" />
    
   
</head>
<body class="o-page">
    <!--[if lte IE 9]>
           <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience and security.</p>
       <![endif]-->
    <!-- SIDEBAR -->
    <div class="o-page__sidebar js-page-sidebar">
        <div class="c-sidebar">
            <a class="c-sidebar__brand" href="#">
                <img class="c-sidebar__brand-img" src="~/Content/img/logo.png" alt="Logo"> System Millon
            </a>
            <h4 class="c-sidebar__title">Panel de Control</h4>
            <ul class="c-sidebar__list">
                <li class="c-sidebar__item">
                    <a class="c-sidebar__link is-active" href="~/Home">
                        <i class="fa fa-home u-mr-xsmall"></i>Inicio
                    </a>
                </li>
                <li class="c-sidebar__item">
                    <a class="c-sidebar__link" href="~/Citas">
                        <i class="fa fa-product-hunt u-mr-xsmall"></i>Citas <span class="c-badge c-badge--success c-badge--xsmall u-ml-xsmall">New</span>
                    </a>
                </li>
                @if (HttpContext.Current.Session["TipoUsuario"].Equals("Paciente") || HttpContext.Current.Session["TipoUsuario"].Equals("administrador"))
                {
                <li class="c-sidebar__item">
                    <a class="c-sidebar__link" href="~/TestMillon">
                        <i class="fa fa-table u-mr-xsmall"></i>Test De Millon
                    </a>
                </li>
                }

                @if (HttpContext.Current.Session["TipoUsuario"].Equals("Psicologo")|| HttpContext.Current.Session["TipoUsuario"].Equals("administrador")) {
                <li class="c-sidebar__item">
                    <a class="c-sidebar__link " href="~/Pacientes">
                        <i class="fa fa-balance-scale u-mr-xsmall"></i>Pacientes
                    </a>
                </li>
                <li class="c-sidebar__item">
                    <a class="c-sidebar__link" href="~/Historias">
                        <i class="fa fa-users u-mr-xsmall"></i>Historias
                    </a>
                </li>
                <li class="c-sidebar__item">
                    <a class="c-sidebar__link"  href="~/Diagnostico">
                        <i class="fa fa-table u-mr-xsmall"></i>Diagnosticos
                    </a>
                </li>
                }
            </ul>
            @if (HttpContext.Current.Session["TipoUsuario"].Equals("administrador"))
            {
            <h4 class="c-sidebar__title">Apps</h4>
            <ul class="c-sidebar__list">
                <li class="c-sidebar__item">
                    <a class="c-sidebar__link"  href="~/Psicologos">
                        <i class="fa fa-money u-mr-xsmall"></i>Psicologos
                    </a>
                </li>
                <li class="c-sidebar__item">
                    <a class="c-sidebar__link" href="~/Administradores">
                        <i class="fa fa-cog u-mr-xsmall"></i>Administradores
                    </a>
                </li>
                <li class="c-sidebar__item">
                    <a class="c-sidebar__link"  href="~/Users">
                        <i class="fa fa-user u-mr-xsmall"></i>Usuarios
                    </a>
                </li>
            </ul>
            }
        </div><!-- // .c-sidebar -->
    </div><!-- // .o-page__sidebar -->
    <!-- MAIN CONTENT -->
    <main class="o-page__content">
        <header class="c-navbar u-mb-medium">
            <button class="c-sidebar-toggle js-sidebar-toggle">
                <span class="c-sidebar-toggle__bar"></span>
                <span class="c-sidebar-toggle__bar"></span>
                <span class="c-sidebar-toggle__bar"></span>
            </button>

            <h2 class="c-navbar__title u-mr-auto">Bienvenido</h2>

            <div class="c-dropdown dropdown u-mr-medium u-ml-small u-hidden-down@mobile">

                <div class="c-dropdown__menu c-dropdown__menu--large dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenuAlerts">
                    <a href="#" class="c-dropdown__item dropdown-item o-media">
                        <span class="o-media__img u-mr-xsmall">
                            <span class="c-avatar c-avatar--xsmall">
                                <span class="c-avatar__img u-bg-success u-flex u-justify-center u-align-items-center">
                                    <i class="fa fa-check u-text-large u-color-white"></i>
                                </span>
                            </span>

                        </span>
                        <div class="o-media__body">
                            <h6 class="u-mb-zero">Completed a task</h6>
                            <p class="u-text-mute">You have recieved a mention on twitter, check it out!</p>
                        </div>
                    </a>
                    <a href="#" class="c-dropdown__item dropdown-item o-media">
                        <span class="o-media__img u-mr-xsmall">
                            <span class="c-avatar c-avatar--xsmall">
                                <span class="c-avatar__img u-bg-fancy u-flex u-justify-center u-align-items-center">
                                    <i class="fa fa-calendar u-text-large u-color-white"></i>
                                </span>
                            </span>

                        </span>
                        <div class="o-media__body">
                            <h6 class="u-mb-zero">Company meetup</h6>
                            <p class="u-text-mute">You have recieved a mention on twitter, check it out!</p>
                        </div>
                    </a>
                    <a href="#" class="c-dropdown__item dropdown-item o-media">
                        <span class="o-media__img u-mr-xsmall">
                            <span class="c-avatar c-avatar--xsmall">
                                <span class="c-avatar__img u-bg-primary u-flex u-justify-center u-align-items-center">
                                    <i class="fa fa-info u-text-large u-color-white"></i>
                                </span>
                            </span>

                        </span>
                        <div class="o-media__body">
                            <h6 class="u-mb-zero">Someone mentioned you</h6>
                            <p class="u-text-mute">You have recieved a mention on twitter, check it out!</p>
                        </div>
                    </a>
                </div>
                <div>
                     @if (HttpContext.Current.Session["Nombres"] == null || HttpContext.Current.Session["ApellidoPaterno"] ==null)
                     {
                         Response.Redirect("~/Account/Index");
                     }
                     else
                     {
                      <h6>@HttpContext.Current.Session["Nombres"] @HttpContext.Current.Session["ApellidoPaterno"] @HttpContext.Current.Session["ApellidoMaterno"]</h6>                
                     }
                </div>

            </div>
            <div class="c-dropdown dropdown">
                <a class="c-avatar c-avatar--xsmall has-dropdown dropdown-toggle" href="#" id="dropdwonMenuAvatar" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <img  src="~/Content/img/users.png"  style="width:40px;height:40px">
                </a>
                <div class="c-dropdown__menu dropdown-menu dropdown-menu-right" aria-labelledby="dropdwonMenuAvatar">
                    <a class="c-dropdown__item dropdown-item" href="#">Editar Perfil</a>
                    <a class="c-dropdown__item dropdown-item" href="@Url.Action("Cerrar", "TestMillon")">Cerrar Sesion</a>

                </div>
            </div>
        </header><!-- // .c-navbar -->
        <div class="container">
            @RenderBody()
        </div><!-- // .container -->
    </main>
    <script>
        var urlGeneral = '@Url.Content("~/")'
    </script>
    <script src="~/Content/js/main.js"></script>
    <script src="~/Content/js/jquery.plugins.js"></script>
    

    @RenderSection("scripts", false)
</body>
</html>
