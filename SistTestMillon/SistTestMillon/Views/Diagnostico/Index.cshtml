@model IEnumerable<Model.Diagnosticos>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row u-mb-large" id="PreguntasTest">
    <div class="col-sm-12">
        <div class="c-table-responsive@desktop">
            <table class="c-table c-table--highlight u-mb-small">
                <caption class="c-table__title">
                    <h3 style="text-align:center">Diagnosticos</h3>
                </caption>
                <thead class="c-table__head c-table__head--slim">
                    <tr class="c-table__row">
                        <th class="c-table__cell c-table__cell--head">N°</th>
                        <th class="c-table__cell c-table__cell--head">Fecha</th>
                        <th class="c-table__cell c-table__cell--head">Hora de Inicio</th>
                        <th class="c-table__cell c-table__cell--head">Hora final</th>
                        <th class="c-table__cell c-table__cell--head">
                            <span class="u-hidden-visually">Tools</span>
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @{int inicio = 0;}
                    @if (Model.Count() > 0)
                    {

                        foreach (var item in Model)
                        {
                            inicio = item.IdDiagnostico;
                            <tr class="c-table__row">
                                <td class="c-table__cell">
                                    <small class="u-block u-text-mute">@item.IdDiagnostico</small>
                                </td>
                                <td class="c-table__cell Pregunta">
                                    @item.Fecha
                                </td>
                                <td class="c-table__cell Pregunta">
                                    @item.HoraInicio
                                </td>
                                <td class="c-table__cell Pregunta">
                                    @item.HoraFinal
                                </td>
                                <td class="c-table__cell u-text-right">
                                    <a class="c-btn c-btn--primary">
                                        <i class="fa fa-pencil-square-o u-mr-small"></i>Eliminar
                                    </a>
                                    @Html.ActionLink("Diagnostico", "ImprimirDetalle", new { id = item.IdDiagnostico }, new { @class ="c-btn c-btn--info", target = "_blank" })
                                </td>

                            </tr>

                        }

                    }
                    else
                    {
                        <tr>
                            <td class="c-table__cell" colspan="10">
                                No tienes productos registrados
                            </td>
                        </tr>
                    }


                </tbody>
            </table>

        </div>
    </div>
</div>
